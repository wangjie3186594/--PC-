  height: 63px;
  text-indent: -9999px;
}
.top .swiper-container .swiper-button-prev {
  background: url(https://s2.music.126.net/style/web2/img/index/banner.png?fe42a61d897a9cd5b4d8593c5d1912e4) 0 9999px no-repeat;
  background-position: 0 -360px;
}
.top .swiper-container .swiper-button-prev:hover {
  display: block;
  background: url(https://s2.music.126.net/style/web2/img/index/banner.png?fe42a61d897a9cd5b4d8593c5d1912e4) 0 9999px no-repeat;
  background-position: 0 -430px;
}
.top .swiper-container .swiper-button-next {
  background: url(https://s2.music.126.net/style/web2/img/index/banner.png?fe42a61d897a9cd5b4d8593c5d1912e4) 0 9999px no-repeat;
  background-position: 0 -508px;
}
.top .swiper-container .swiper-button-next:hover {
  display: block;
  background: url(https://s2.music.126.net/style/web2/img/index/banner.png?fe42a61d897a9cd5b4d8593c5d1912e4) 0 9999px no-repeat;
  background-position: 0 -578px;
}
.down {
  position: absolute;
  top: 0;
  z-index: 20;
  right: 17.8%;
  width: 254px;
  height: 336px;
  background: url(../../assets/image/download.png) no-repeat 0 0;
}
.down .onload {
  display: block;
  width: 215px;
  height: 56px;
  margin: 212px 0 0 19px;
  background-position: 0 9999px;
  text-indent: -9999px;
}
.down p {
  margin: 10px auto;
  text-align: center;
  color: #8d8d8d;
  font-size: 12px;
}
.content {
