  background-position: 0 0;
}
.content .fg_rig .info p {
  width: 205px;
  margin: 0 auto;
  padding: 16px 0;
  line-height: 22px;
  font-size: 12px;
  color: #666;
}
.content .fg_rig .info a {
  display: block;
  width: 100px;
  height: 31px;
  line-height: 31px;
  text-align: center;
  color: #fff;
  text-shadow: 0 1px 0 #8a060b;
  margin: 0 auto;
  font-size: 12px;
  background: url(https://s2.music.126.net/style/web2/img/index/index.png?0bab0a6ba8c1d067b1bc17701c7a1d67) no-repeat 0 9999px;
  background-position: 0 -195px;
}
.content .fg_rig .Hideinfo {
  height: 200px;
  padding-top: 20px;
  background: url(https://s2.music.126.net/style/web2/img/index/index.png?0bab0a6ba8c1d067b1bc17701c7a1d67) no-repeat 0 9999px;
  background-position: 0 -270px;
}
.content .fg_rig .Hideinfo .portrait {
  float: left;
  width: 80px;
  height: 80px;
  margin-left: 20px;
  padding: 2px;
  background: #fff;
  border: 1px solid #dadada;
}
.content .fg_rig .Hideinfo .portrait img {
  display: block;
  width: 100%;
  height: 100%;
}
.content .fg_rig .Hideinfo .fcb {
  float: left;
  width: 115px;
  margin-left: 18px;
  padding-top: 3px;
}
.content .fg_rig .Hideinfo .fcb .name {
  overflow: hidden;
}
.content .fg_rig .Hideinfo .fcb .name a {
  width: auto;
  max-width: 100%;
  vertical-align: middle;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
  font-size: 14px;
  color: #000;
  font-weight: bold;
}
.content .fg_rig .Hideinfo .fcb .grade {
  width: 100%;
  margin-top: 5px;
}
.content .fg_rig .Hideinfo .fcb .grade a {
  display: inline-block;
  height: 17px;
  overflow: hidden;
  padding-left: 25px;
  line-height: 18px;
  color: #999;
  font-weight: bold;
  font-style: italic;
  background: url(https://s2.music.126.net/style/web2/img/icon2.png?5362470711c8fc1182efe7944ef80716) no-repeat 0 9999px;
  background-position: -130px -64px;
}
.content .fg_rig .Hideinfo .fcb .grade a i {
  display: inline-block;
  overflow: hidden;
  vertical-align: middle;
  float: right;
  width: 8px;
  height: 17px;
  background: url(https://s2.music.126.net/style/web2/img/icon2.png?5362470711c8fc1182efe7944ef80716) no-repeat 0 9999px;
  background-position: -192px -64px;
}
.content .fg_rig .Hideinfo .fcb .butto {
  position: relative;
  margin-top: 15px;
  zoom: 1;
}
.content .fg_rig .Hideinfo .fcb .butto a {
  display: inline-block;
  color: #fff;
  padding: 0 5px 0 0;
  white-space: nowrap;
  height: 31px;
  line-height: 31px;
  overflow: hidden;
  vertical-align: top;
  text-align: center;
  cursor: pointer;
  background: url(https://s2.music.126.net/style/web2/img/button2.png?11b102c6a06ee1d31116b94bc2bf6128) no-repeat 0 9999px;
  background-position: right -428px;
}
.content .fg_rig .Hideinfo .fcb .butto a i {
  font-size: 12px;
  display: inline-block;
  padding: 0 15px 0 20px;
  pointer-events: none;
  width: 60px;
  color: #fff;
  background: url(https://s2.music.126.net/style/web2/img/button2.png?11b102c6a06ee1d31116b94bc2bf6128) no-repeat 0 9999px;
  background-position: 0 -387px;
}
.content .fg_rig .Hideinfo .fcb::after {
  clear: both;
  content: '.';
  display: block;
  height: 0;
  visibility: hidden;
}
.content .fg_rig .Hideinfo ul {
  margin: 12px 0 0 12px;
  color: #666;
}
.content .fg_rig .Hideinfo ul li {
  float: left;
  height: 40px;
  padding: 0 18px;
  border-right: 1px solid #e4e4e4;
}
.content .fg_rig .Hideinfo ul li a {
  display: block;
  color: #666;
  font-size: 12px;
}
.content .fg_rig .Hideinfo ul li a strong {
  display: block;
  font-size: 20px;
  font-weight: normal;
}
.content .fg_rig .Hideinfo ul li a span {
  margin-left: 2px;
}
.content .fg_rig .Hideinfo ul li.firs {
  padding-left: 0;
}
.content .fg_rig::after {
  content: '';
  clear: both;
  display: block;
  height: 0;
  visibility: hidden;
}
